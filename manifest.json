{
    "manifest_version" : 2,
    "name" : "Dater",
    "version" : "1.0.0",
    "content_scripts" : [
        {
            "matches" : ["https://tinder.com/*"],
            "js" : [
                "swiper.js"
            ]
        }
    ],
    "permissions":[
        "tabs"
    ],
    "browser_action": {
        "default_popup" : "popup.html",
        "default_icon": {
            "16": "images/icon16.png",
            "32": "images/icon32.png",
            "48": "images/icon48.png",
            "128": "images/icon128.png"
        }
    },
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "icons": {
        "16": "images/icon16.png",
        "32": "images/icon32.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    }
}